
<div class="panel-body">

	<%= form_for @chanel, html: { multipart: true } do |f| %><br>

		<div class="form-group">
				<%= f.label :image %><br>
				<%= f.file_field :image, class: "form-control" %><br>
			</div>
			
			
			<div class="form-group">
		          <%= f.submit "Submit", class: "btn btn-warning" %>
		    </div>
			
				<% end %>
		</div>
</div>